generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Product {
  id            String @id @default(auto()) @map("_id") @db.ObjectId
  name          String @unique
  imageUrl      String
  category      String
  protein       Float
  carbohydrates Float
  fat           Float
  fiber         Float
}
